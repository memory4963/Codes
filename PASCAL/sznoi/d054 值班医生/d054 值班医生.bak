var
a,b,c,d,e,f,g,n,i:integer;
procedure pc(m:integer);
begin
case m of
 1:writeln('mon');
 2:writeln('tue');
 3:writeln('wed');
 4:writeln('thu');
 5:writeln('fri');
 6:writeln('sat');
 7:writeln('sun');
end;
end;
begin
f:=4;
for a:= 1 to 7 do
 for b:= 1 to 7 do
  for c:= 1 to 7 do
   for d:= 1 to 7 do
    for e:= 1 to 7 do
      for g:= 1 to 7 do
        if (a=b+1) and (d=e+2) and ((b=g-3) or (b=g+4)) and (f>b) and (f<c) then
        begin
         write('a:'); pc(a);
         write('b:'); pc(b);
         write('c:'); pc(c);
         write('d:'); pc(d);
         write('e:'); pc(e);
         write('f:'); pc(f);
         write('g:'); pc(g);
        end
end.
var
n,i,j,k:longint;
a:array[1..11,1..11] of longint;
b:array[1..11,1..11] of boolean;
begin
readln(n);
fillchar(b,sizeof(b),true);
i:=1;
j:=(n div 2 +1);
a[i,j]:=1;
for k:= 2 to (n*n) do
 begin
  if (i-1=0) or (j-1=0) then
  begin
   if (i-1=0) and (b[n,j-1]) then
   begin
   a[n,j-1]:=k;
   b[n,j-1]:=false;
   i:=n;
   j:=j-1;
   end
   else if (j-1=0) and (b[i-1,n]) then
    begin
    a[i-1,n]:=k;
    b[i-1,n]:=false;
    i:=i-1;
    j:=n;
    end
    else a[i+1,j]:=k;
 end
 else if b[i-1,j-1] then
 begin
 a[i-1,j-i]:=k;
 b[i-1,j-1]:=false;
 i:=i-1;
 j:=j-1;
 end
 else
 begin
 a[i+1,j]:=k;
 b[i+1,j]:=false;
 i:=i+1;
 j:=j;
 end;
 end;
for i:= 1 to n do
 begin
  for j:= 1 to n do
    write(a[i,j],' ');
  writeln;
 end;
end.


